{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "esModuleInterop": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@helpers/*": [
        "./src/helpers/*"
      ],
      "@model/*": [
        "./src/model/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@filth/*": [
        "./src/lib/*"
      ]
    }
  }
}