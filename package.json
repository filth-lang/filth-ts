{
  "name": "filth",
  "private": false,
  "version": "0.2.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx --bun vite build",
    "build:site": "bunx --bun vite build --config vite.site.config.ts",
    "build:grammar": "bun scripts/generate_parser.ts",
    "watch:grammar": "bun scripts/generate_parser.ts --watch",
    "lint:format": "prettier --cache --check .",
    "preview": "bunx --bun vite preview",
    "lint": "eslint --cache .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "outdated": "bunx npm-check-updates --interactive --format group",
    "tsc": "tsc --noEmit",
    "check": "npm-run-all --parallel tsc lint lint:format",
    "knip": "knip",
    "prepare": "bunx --bun vite build"
  },
  "dependencies": {},
  "devDependencies": {
    "peggy": "^4.2.0",
    "ts-pegjs": "^4.2.1",
    "@eslint/js": "^9.23.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@nkzw/eslint-config": "^2.1.0",
    "@preact/preset-vite": "^2.10.1",
    "@types/bun": "^1.2.6",
    "@types/node": "^22.13.14",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "jotai": "^2.12.2",
    "knip": "^5.46.2",
    "npm-run-all": "^4.1.5",
    "preact": "^10.26.4",
    "prettier": "^3.5.3",
    "superjson": "^2.2.2",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.3",
    "vite-plugin-dts": "^3.7.3"
  }
}